<zapaddon>
	<name>Active scanner rules (beta)</name>
	<version>22</version>
	<status>beta</status>
	<description>The beta quality Active Scanner rules</description>
	<author>ZAP Dev Team</author>
	<url/>
	<changes>
	<![CDATA[
	Fix FP in "Source Code Disclosure - /WEB-INF folder" on successful responses (Issue 3048).<br>
	Fix FP in "Integer Overflow Error" on 500 error responses (Issue 3064).<br>
	Support security annotations for forms that dont need anti-CSRF tokens.<br>
	Changed XXE rule to use new callback extension.<br>
	Notify of messages sent during Heartbleed scanning (Issue 2425).<br>
	Fix false positive in Code Disclosure - CVE-2012-1823 on image content (Issue 3846).<br>
	Fix false positive in Backup File Disclosure scanner on 403 responses (Issue 3911).<br>
	CsrfTokenScan : Keep session cookies instead of deleting all of them<br>
    ]]>
	</changes>
	<extensions>
		<extension>org.zaproxy.zap.extension.ascanrulesBeta.ExtensionAscanRulesBeta</extension>
	</extensions>
	<ascanrules>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.BackupFileDisclosure</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.CrossDomainScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.Csrftokenscan</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.ExpressionLanguageInjectionPlugin</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.HeartBleedActiveScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.HPP</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.InsecureHTTPMethod</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.IntegerOverflow</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.PaddingOraclePlugin</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.RemoteCodeExecutionCVE20121823</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SessionFixation</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.ShellShockScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionHypersonic</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionMySQL</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionOracle</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionPostgresql</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureCVE20121823</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureSVN</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureWEBINF</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.UsernameEnumeration</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.XpathInjectionPlugin</ascanrule>                
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.XXEPlugin</ascanrule>
	</ascanrules>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.6.0</not-before-version>
	<not-from-version/>
</zapaddon>

